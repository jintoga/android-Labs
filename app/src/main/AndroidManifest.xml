<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.dat.mylabs"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Lab1.Lab1FirstActivity" />
        <activity android:name=".Lab1.Lab1SecondActivity" />
        <activity android:name=".Lab2.Lab2FirstActivity" />
        <activity android:name=".Lab2.Lab2SecondActivity" />
        <activity android:name=".Lab3.Lab3FirstActivity" />

        <service
            android:name=".Lab3.MyService"
            android:enabled="true"
            android:exported="true" />
        <service
            android:name=".Lab3.MyIntentService"
            android:exported="false" />
        <service
            android:name=".Lab3.MyNotStickyService"
            android:enabled="true"
            android:exported="true" />

        <activity android:name=".Lab3.Lab3SecondActivity" />

    </application>

</manifest>